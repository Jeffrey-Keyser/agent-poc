{
  "name": "openai-agents-poc",
  "version": "1.0.0",
  "description": "OpenAI Agents SDK TypeScript Proof of Concept",
  "main": "agent-poc.ts",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node agent-poc.ts",
    "start:openator": "ts-node agent-openator-poc.ts",
    "start:grubhub": "ts-node agent-grubhub-poc.ts",
    "start:amazon": "ts-node agent-amazon-poc.ts",
    "start:github": "ts-node agent-github-poc.ts",
    "start:amazon-multi": "ts-node agent-amazon-multi.ts",
    "dev": "ts-node --watch agent-poc.ts",
    "build": "tsc",
    "build:run": "npm run build && node dist/agent-poc.js",
    "clean": "rm -rf dist",
    "test": "echo \"No tests defined yet\" && exit 0",
    "postinstall": "playwright install chromium",
    "install:browsers": "playwright install"
  },
  "keywords": [
    "openai",
    "agents",
    "typescript",
    "ai",
    "proof-of-concept"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.19",
    "@langchain/openai": "^0.4.4",
    "@mozilla/readability": "^0.5.0",
    "class-validator": "^0.14.1",
    "dom-to-semantic-markdown": "^1.3.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "jsdom": "^26.0.0",
    "jsondom": "^0.4.1",
    "langchain": "^0.3.19",
    "playwright": "^1.55.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
